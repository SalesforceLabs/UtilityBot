                <extractionRegex>\bAN\-\d{8}\b</extractionRegex>
                <extractionType>Pattern</extractionType>
                <label>Ent_AccountNumber</label>
            </mlSlotClasses>
            <mlSlotClasses>
                <description>Email pattern</description>
                <developerName>Ent_Email</developerName>
                <extractionRegex>\b[a-zA-Z0-9.!#$%&amp;’*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)*\b</extractionRegex>
                <extractionType>Pattern</extractionType>
                <label>Ent_Email</label>
            </mlSlotClasses>
            <mlSlotClasses>
