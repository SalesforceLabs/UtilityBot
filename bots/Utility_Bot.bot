                <developerName>Ent_Phone</developerName>
                <extractionRegex>\b(1\s?)?((\([0-9]{3}\))|[0-9]{3})[\s\-]?[0-9]{3}[\s\-]?[0-9]{4}\b</extractionRegex>
                <extractionType>Pattern</extractionType>
